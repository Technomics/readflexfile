citHeader("To cite readflexfile in publications use:")

citEntry(
  entry    = "Article",
  title    = "Importing the FlexFile with {readflexfile}",
  author   = personList(as.person("Benjamin Berkman"),
                   as.person("Justin Cooper"),
                   as.person("Adam James")),
  journal  = ,
  year     = "2020",
  volume   = ,
  number   = ,
  pages    = ,
  url      = ,
  textVersion = paste("Benjamin Berkman, Justin Cooper, Adam James (2020).",
        "Importing the FlexFile with readflexfile."
  )
)
